<Window x:Class="DataGrid_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataGrid_WPF"
        xmlns:FastGrid_Data ="clr-namespace:DataGrid_WPF.DataGrid_Fast"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <FastGrid_Data:FastGridControl x:Name="grid1" Grid.Row="1" IsReadOnly="True">
            </FastGrid_Data:FastGridControl>
        </Grid>
    </Grid>
</Window>
